    "www-jquery.qtip": {
      "src": "./resources/assets/js/www/vendor/plugins/jquery.qtip.js",
      "dst": "./public/js/www/vendor/",
      "name": "jquery.qtip.js",
      "watch": "./resources/assets/js/www/vendor/plugins/jquery.qtip.js"
    },
    "guests-main": {
      "src": "./resources/assets/js/guests/main.js",
      "dst": "./public/js/guests/",
      "name": "main.js",
      "watch": [
        "./resources/assets/js/guests/**/*.js",
        "!./resources/assets/js/guests/vendor/leaflet.js",
        "!./resources/assets/js/guests/vendor/leaflet.extra-markers.js"
      ]
    },
    "guests-leaflet": {
      "src": "./resources/assets/js/guests/vendor/leaflet.js",
      "dst": "./public/js/guests/vendor/",
      "name": "leaflet.js",
      "watch": "./resources/assets/js/guests/vendor/leaflet.js"
    },
    "guests-leaflet.extra-markers": {
      "src": "./resources/assets/js/guests/vendor/leaflet.extra-markers.js",
      "dst": "./public/js/guests/vendor/",
      "name": "leaflet.extra-markers.js",
      "watch": "./resources/assets/js/guests/vendor/leaflet.extra-markers.js"
    },
    "guests-jquery.qtip": {
      "src": "./resources/assets/js/guests/vendor/plugins/jquery.qtip.js",
      "dst": "./public/js/guests/vendor/",
      "name": "jquery.qtip.js",
      "watch": "./resources/assets/js/guests/vendor/plugins/jquery.qtip.js"
    }
  },
  "images": {
    "root": {
      "src": "./public/img/**/*",
      "dst": "./public/img/",
      "watch": "./public/img/**/*"
    }
  },
  "knownOptions": {
    "default": {
      "production": false,
      "skipcache": false
    }
  }
}
