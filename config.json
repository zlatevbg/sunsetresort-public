{
  "styles": {
    "cms-main": {
      "src": "./resources/assets/sass/cms/main.scss",
      "dst": "./public/css/cms/",
      "name": "main.css",
      "watch": "./resources/assets/sass/cms/**/*.scss"
    },
    "www-main": {
      "src": "./resources/assets/sass/www/main.scss",
      "dst": "./public/css/www/",
      "name": "main.css",
      "watch": "./resources/assets/sass/www/**/*.scss"
    },
    "guests-main": {
      "src": "./resources/assets/sass/guests/main.scss",
      "dst": "./public/css/guests/",
      "name": "main.css",
      "watch": "./resources/assets/sass/guests/**/*.scss"
    }
  },
  "scripts": {
    "cms-main": {
      "src": "./resources/assets/js/cms/main.js",
      "dst": "./public/js/cms/",
      "name": "main.js",
      "watch": [
        "./resources/assets/js/cms/**/*.js",
        "!./resources/assets/js/cms/vendor/leaflet.js",
        "!./resources/assets/js/cms/vendor/leaflet.extra-markers.js",
        "!./resources/assets/js/cms/vendor/fine-uploader.js",
        "!./resources/assets/js/cms/vendor/ckfinder-3.2.0.js",
        "!./resources/assets/js/cms/vendor/plugins/jquery.multiselect.js",
        "!./resources/assets/js/cms/vendor/plugins/jquery.qtip.js",
        "!./resources/assets/js/cms/vendor/jquery-ui.js"
      ]
    },
    "cms-leaflet": {
      "src": "./resources/assets/js/cms/vendor/leaflet.js",
      "dst": "./public/js/cms/vendor/",
      "name": "leaflet.js",
      "watch": "./resources/assets/js/cms/vendor/leaflet.js"
    },
    "cms-leaflet.extra-markers": {
      "src": "./resources/assets/js/cms/vendor/leaflet.extra-markers.js",
