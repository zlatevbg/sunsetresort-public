      "watch": "./resources/assets/js/cms/vendor/leaflet.extra-markers.js"
    },
    "cms-fine-uploader": {
      "src": "./resources/assets/js/cms/vendor/fine-uploader.js",
      "dst": "./public/js/cms/vendor/",
      "name": "fine-uploader.js",
      "watch": "./resources/assets/js/cms/vendor/fine-uploader.js"
    },
    "cms-ckfinder-3.2.0": {
      "src": "./resources/assets/js/cms/vendor/ckfinder-3.2.0.js",
      "dst": "./public/js/cms/vendor/ckfinder/",
      "name": "ckfinder.js",
      "watch": "./resources/assets/js/cms/vendor/ckfinder-3.2.0.js"
    },
    "cms-jquery.multiselect": {
      "src": "./resources/assets/js/cms/vendor/plugins/jquery.multiselect.js",
      "dst": "./public/js/cms/vendor/",
      "name": "jquery.multiselect.js",
      "watch": "./resources/assets/js/cms/vendor/plugins/jquery.multiselect.js"
    },
    "cms-jquery-ui": {
      "src": "./resources/assets/js/cms/vendor/jquery-ui.js",
