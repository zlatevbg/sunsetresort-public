      "name": "jquery-ui.js",
      "watch": "./resources/assets/js/cms/vendor/jquery-ui.js"
    },
    "www-main": {
      "src": "./resources/assets/js/www/main.js",
      "dst": "./public/js/www/",
      "name": "main.js",
      "watch": [
        "./resources/assets/js/www/**/*.js",
        "!./resources/assets/js/www/vendor/leaflet.js",
        "!./resources/assets/js/www/vendor/leaflet.extra-markers.js",
        "!./resources/assets/js/www/vendor/plugins/jquery.multiselect.js"
      ]
    },
    "www-leaflet": {
      "src": "./resources/assets/js/www/vendor/leaflet.js",
      "dst": "./public/js/www/vendor/",
      "name": "leaflet.js",
      "watch": "./resources/assets/js/www/vendor/leaflet.js"
